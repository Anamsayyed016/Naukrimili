{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "buildCommand": "next build",
  "devCommand": "next dev",
  "installCommand": "pnpm install",
  "functions": {
    "app/api/**/*": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/feedback/:id",
      "destination": "/api/feedback/:id"
    },
    {
      "source": "/feedback/((?!general).*)",
      "destination": "/api/feedback/general"
    }
  ]
}
