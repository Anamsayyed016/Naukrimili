{
  "framework": "nextjs",
  "buildCommand": "pnpm install --no-frozen-lockfile && pnpm build",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "env": {
    "NEXT_PUBLIC_MOCK_DATA": "true",
    "NEXT_PUBLIC_DISABLE_AUTH": "false",
    "NODE_ENV": "production",
    "NEXT_PUBLIC_API_URL": "/api"
  }
}