    "db:studio": "prisma studio",
    "db:seed": "npx ts-node scripts/seed-data.ts",
    "db:seed:users": "npx ts-node scripts/seed-data.ts --type=users --count=50",
    "db:seed:jobs": "npx ts-node scripts/seed-data.ts --type=jobs --count=100",
    "db:seed:companies": "npx ts-node scripts/seed-data.ts --type=companies --count=20",
    "db:seed:all": "npx ts-node scripts/seed-data.ts --type=all --count=50",
    "db:seed:prod": "npx ts-node scripts/seed-data.ts --production-safe",
    "env:setup": "npx ts-node scripts/setup-env.ts",
    "env:check": "npx ts-node scripts/setup-env.ts",
    "report:daily": "npx ts-node scripts/cron-jobs/daily-report.ts",
    "cleanup:logs": "npx ts-node scripts/cron-jobs/clean-old-logs.ts",
    "cleanup:logs:dry": "npx ts-node scripts/cron-jobs/clean-old-logs.ts --dry-run",
    "test:resume": "node test-resume-upload.js",
    "deploy:prod": "npm run build && npm start",
    "deploy:pm2": "pm2 start ecosystem.config.cjs --env production",
    "deploy:pm2:reload": "pm2 reload ecosystem.config.cjs --env production",
    "deploy:pm2:stop": "pm2 stop jobportal",
    "deploy:pm2:logs": "pm2 logs jobportal",
    "deploy:pm2:monit": "pm2 monit",
    "deploy:fast": "bash deploy-fast.sh",
    "deploy:server": "git pull origin main && npm ci --only=production --legacy-peer-deps && npm run build:ultra-fast && pm2 restart jobportal",
    "deploy:ultra": "bash deploy-server.sh",
    "deploy:quick": "bash quick-deploy.sh",
    "deploy:windows": "powershell -ExecutionPolicy Bypass -File deploy-server.ps1",
    "deploy:fix": "bash scripts/deploy-production-fix.sh",
    "deploy:fix:linux": "bash scripts/deploy-fix-complete.sh",
    "deploy:fix:windows": "node scripts/deploy-production-fix.cjs",
    "start:production": "set NODE_ENV=production && node server.cjs",
    "pm2:start": "pm2 start ecosystem.config.cjs --env production",
    "pm2:stop": "pm2 stop jobportal",
    "pm2:restart": "pm2 restart jobportal",
    "pm2:logs": "pm2 logs jobportal",
    "pm2:status": "pm2 status",
    "clean": "rm -rf .next out production",
    "production:build": "npm ci --only=production --legacy-peer-deps && npm run build",
