    "build:ci": "rm -f package-lock.json && echo 'engine-strict=false' > .npmrc && echo 'legacy-peer-deps=true' >> .npmrc && echo 'fund=false' >> .npmrc && echo 'audit=false' >> .npmrc && npm install --legacy-peer-deps --engine-strict=false --force && npm install tailwindcss postcss autoprefixer @radix-ui/react-slot @radix-ui/react-dialog @radix-ui/react-dropdown-menu @radix-ui/react-toast class-variance-authority clsx tailwind-merge lucide-react --legacy-peer-deps --engine-strict=false && npx prisma generate && NODE_ENV=production NODE_OPTIONS=--max-old-space-size=4096 npm run build && echo $(date +%s) > .next/BUILD_ID",
    "build:production": "bash build-production.sh",
    "build:cache-bust": "./scripts/deploy-with-cache-bust.sh",
    "build:fast": "NODE_OPTIONS=\"--max-old-space-size=4096\" NEXT_TELEMETRY_DISABLED=1 next build --no-lint",
    "build:optimized": "node optimize-build.js",
    "build:ultra-fast": "NODE_OPTIONS=\"--max-old-space-size=8192\" NEXT_TELEMETRY_DISABLED=1 next build --no-lint",
    "build:deploy": "NODE_ENV=production NODE_OPTIONS=\"--max-old-space-size=4096\" NEXT_TELEMETRY_DISABLED=1 next build",
    "build:simple": "NODE_ENV=production next build",
    "build:minimal": "next build --no-lint",
    "validate:build": "node scripts/validate-build-artifacts.cjs",
    "start": "next start",
    "start:socket": "node server.js",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:role-selection": "jest tests/role-selection.test.tsx tests/api-set-role.test.ts",
    "test:role-selection:integration": "jest tests/role-selection-integration.test.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "npx ts-node scripts/migrate-db.ts --force-production",
